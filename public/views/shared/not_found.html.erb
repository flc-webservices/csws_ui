<div id="main-content" class="row">
<% Rails.logger.debug("****NOT FOUND: title: #{@page_title}  URI: #{@uri} back: #{@back_uri}") %>

 <div class="warning col-sm-12">
   <h2><%= t("errors.error_404_message", :type => @type) %></h2>
   <p><b><%= @uri %></b></p>
   <% unless @back_url.blank? %>
   <p><a href="<%= @back_url %>" title="<%= t("actions.backup_abbr") %>"><%= t("actions.backup") %></a></p>
   <% end %>
 </div>
</div>
